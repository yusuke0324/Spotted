<div class="register-photo">
<div class="form-container">
    <div class="image-holder"></div>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>
        <h2 class="text-center"><strong>Create</strong> an account.</h2>
        <div class="form-group">
          <%= f.text_field :first_name, autofocus: true, class:"form-control", placeholder: "First Name" %>
        </div>
        <div class="form-group">
            <%= f.text_field :last_name, autofocus: true, class:"form-control",placeholder: "Last Name" %>
        </div>
        <div class="form-group">
            <%= f.text_field :username, autofocus: true, class:"form-control", placeholder: "User name" %>
        </div>
        <div class="form-group">
            <%= f.text_field :email, autofocus: true, class:"form-control", placeholder: "Email" %>
        </div>
        <div class="form-group">
            <%= f.text_field :phone, autofocus: true, class:"form-control", placeholder: "Phone number" %>
        </div>
        <div class="form-group">
            <% if @minimum_password_length %>
            <em>(<%= @minimum_password_length %> characters minimum)</em>
            <% end %>
            <%= f.password_field :password, autocomplete: "off",class:"form-control" %>
        </div>
        <div class="form-group">
            <%= f.password_field :password_confirmation, autocomplete: "off",class:"form-control" %>
        </div>

        <div class="form-group">
            <div class="checkbox">
                <!-- <label class="control-label"> -->
                    <!-- <input type="checkbox" />I agree to the license terms.</label> -->
            </div>
        </div>
        <div class="form-group">
            <button class="btn btn-info btn-block" type="submit"><%= f.submit "Sign up" %></button>
        </div><a href="/users/sign_in" class="already">You already have an account? Login here.</a>
        <% end %>
</div>
</div>

